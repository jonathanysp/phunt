extends layout
block head
  script(src="/socket.io/socket.io.js")
  script(src="/js/socket.js")
  script(src="/js/leaderboard.js")

block content
  h2 Game ID: #{gameid}
  div(id="progressBars")
  table.table(id="table", border = "2")
    thead
      tr(id="headingRow")
        th TASKS
    tbody
      each task in g.tasks
        tr(class="taskRow")
          td #{task}
  script.
    console.log("hi");
    addLeaderboardEvents();
    register(#{"'" + gameid + "'"}, null);
    getInfo(#{"'" + gameid + "'"});